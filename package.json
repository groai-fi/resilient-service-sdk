{
    "name": "@groai-fi/resilient-service-sdk",
    "version": "1.0.0",
    "description": "A resilience-first SDK for backend services including circuit breakers, HTTP clients, and caching.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
        "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "resilience",
        "circuit-breaker",
        "redis",
        "cache",
        "http"
    ],
    "author": "Hamilton Wang",
    "license": "Apache-2.0",
    "engines": {
        "node": ">=20.0.0"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/groai-fi/resilient-service-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/groai-fi/resilient-service-sdk/issues"
    },
    "homepage": "https://github.com/groai-fi/resilient-service-sdk#readme",
    "dependencies": {
        "axios": "^1.6.0",
        "axios-retry": "^3.8.0",
        "node-cache": "^5.1.2",
        "opossum": "^8.1.0",
        "redis": "^4.6.0"
    },
    "devDependencies": {
        "@types/jest": "^29.0.0",
        "@types/node": "^20.0.0",
        "@types/node-cache": "^4.1.3",
        "@types/opossum": "^8.1.9",
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.55.0",
        "axios-mock-adapter": "^1.22.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "jest": "^29.0.0",
        "prettier": "^3.8.1",
        "ts-jest": "^29.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.55.0"
    }
}
